<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.halkomills.moodmap" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.halkomills.moodmap.DashboardActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.halkomills.moodmap.StatisticsActivity"
            android:label="@string/title_activity_statistics" >
        </activity>
        <activity
            android:name="com.halkomills.moodmap.SelectMoodActivity"
            android:label="@string/title_activity_select_mood" >
        </activity>
    </application>

</manifest>
