<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.halkomills.moodmap" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/logoicon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.halkomills.moodmap.DashboardActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.halkomills.moodmap.StatisticsActivity"
            android:parentActivityName="com.halkomills.moodmap.DashboardActivity"
            android:label="@string/title_activity_statistics" >
        </activity>
        <activity
            android:name="com.halkomills.moodmap.MoodLog.LogActivity"
            android:parentActivityName="com.halkomills.moodmap.DashboardActivity"
            android:label="@string/title_activity_log" >
        </activity>
        <activity
            android:name="com.halkomills.moodmap.CustomMoodActivity"
            android:label="@string/title_activity_custom_mood"
            android:parentActivityName="com.halkomills.moodmap.DashboardActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.halkomills.moodmap.DashboardActivity" />
        </activity>
    </application>

</manifest>
